<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	
	<g:HTMLPanel tag="span">
	<div class="input-append datebox-relative">
		<div class="date" ui:field="dateDiv">
			<g:TextBox ui:field="dateTextBox" styleName="span2" />
			<span class="add-on addon-icon">
				<i ui:field="dateIcon" class="icon-calendar"></i>
			</span>
		</div>
			<span class="addon-icon datebox-relative-clearfield">
				<g:Anchor ui:field="removeIcon">
				<i ui:field="removeIconEl" class="icon-remove datebox-relative-icon"></i>
				</g:Anchor>
			</span>
</div>
	</g:HTMLPanel>
</ui:UiBinder>