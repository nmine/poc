<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:with field="valueboxLabel" type="java.lang.String" />
	<g:HTMLPanel>
		<div class="control-group" ui:field="valueboxDiv">
			<label class="control-label">
				<g:InlineHTML text="test" />
			</label>
			<div class="controls">
				<g:Widget ui:field="valuebox" />
				<span class="help-inline hidden" ui:field="valueboxErrorField">
					<g:InlineLabel ui:field="valueboxErrorFieldLabel" />
				</span>
			</div>
		</div>
	</g:HTMLPanel>
</ui:UiBinder>